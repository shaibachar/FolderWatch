version: '3'
services:
  eureka-server:
    image: eureka-server
    ports:
     - "8761:8761"
  fc-server:
    image: fc-server
    depends_on:
     - eureka-server
    ports:
     - "9090:2222"
  fc-server:
    image: fc-server
    depends_on:
     - eureka-server
     - fca-server
    ports:
     - "9091:3333"
